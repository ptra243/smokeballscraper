<Application x:Class="Scraper.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:viewmodels="clr-namespace:Scraper.Presentation.ViewModels"
             xmlns:views="clr-namespace:Scraper.Presentation.Views.Search"
             xmlns:local="clr-namespace:Scraper">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/MainTheme.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
            <DataTemplate DataType="{x:Type viewmodels:SearchViewModel}">
                <views:Search />
            </DataTemplate>
        </ResourceDictionary>
    </Application.Resources>
</Application>
